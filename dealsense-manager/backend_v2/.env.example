# Discord Webhook Configuration
# Set this to true to enable Discord logging
DISCORD_LOGGING_ENABLED=false

# Set this to true to enable detailed Gemini API call logging to Discord
DISCORD_GEMINI_LOGGING_ENABLED=false

# Discord webhook URLs for different log levels
# You can create these webhooks in your Discord server settings
DISCORD_INFO_WEBHOOK=https://discord.com/api/webhooks/YOUR_INFO_WEBHOOK_URL
DISCORD_WARN_WEBHOOK=https://discord.com/api/webhooks/YOUR_WARN_WEBHOOK_URL
DISCORD_ERROR_WEBHOOK=https://discord.com/api/webhooks/YOUR_ERROR_WEBHOOK_URL
DISCORD_DEBUG_WEBHOOK=https://discord.com/api/webhooks/YOUR_DEBUG_WEBHOOK_URL

# Special webhook for Gemini API calls (requests and responses)
# This will receive detailed logs of every Gemini API interaction
DISCORD_GEMINI_WEBHOOK=https://discord.com/api/webhooks/YOUR_GEMINI_WEBHOOK_URL

# Discord bot username (optional, defaults to "Joinly Bot")
DISCORD_BOT_USERNAME=Joinly Bot

# Standard logging configuration
LOG_LEVEL=debug
LOG_FORMAT=json

# Server configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8001

# Joinly configuration
JOINLY_URL=http://135.235.237.143:8000/mcp/
MAX_AGENTS=10